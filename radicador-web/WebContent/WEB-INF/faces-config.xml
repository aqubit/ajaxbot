<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0" metadata-complete="false">
	<navigation-rule>
		<from-view-id>/pages/radweb.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{captcha.okPressedCaptcha}</from-action>
			<from-outcome>radicar</from-outcome>
			<to-view-id>/pages/visualizarRadicacion?codigoExpediente=NGG-11151</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	<application>
		<locale-config>
			<default-locale>en</default-locale>
			<supported-locale>en</supported-locale>
		</locale-config>
		<resource-bundle>
			<base-name>com.ccx.ingeominasapp.view.resources.messages</base-name>
			<var>msg</var>
		</resource-bundle>
	</application>
</faces-config>
